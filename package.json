{
  "name": "poker-tracker",
  "version": "1.0.0",
  "description": "Mobile-friendly poker session tracker",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"tsc -w\" \"nodemon dist/server.js\"",
    "build": "tsc",
    "start": "node dist/server.js",
    "setup": "npm install && npm run build"
  },
  "keywords": [
    "poker",
    "tracker",
    "sessions"
  ],
  "author": "Poker Player",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "sqlite3": "^5.1.6",
    "multer": "^1.4.5-lts.1",
    "csv-parse": "^5.5.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.18",
    "@types/express-session": "^1.17.10",
    "@types/node": "^20.8.0",
    "@types/sqlite3": "^3.1.8",
    "@types/multer": "^1.4.8",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
